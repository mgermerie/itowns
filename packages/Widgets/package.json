{
  "private": true,
  "name": "@test-itowns-npm-token/widgets",
  "version": "2.46.0",
  "description": "Widgets",
  "type": "module",
  "main": "lib/Main.js",
  "exports": {
    ".": "./lib/Main.js"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "copy_transpile": "npx copyfiles -u 1 \"./lib/**/*\" ../Main/lib/Utils/gui/",
    "transpileOnly": "cross-env BABEL_DISABLE_CACHE=1 babel src --out-dir lib --extensions .js,.ts",
    "transpile": "npm run transpileOnly && npm run copy_transpile",
    "watch": "npm run transpileOnly -- --watch",
    "version": "cross-env-shell npm install @test-itowns-npm-token/geographic@$npm_package_version test-itowns-npm-token@$npm_package_version"
  },
  "files": [
    "*.md",
    "src",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iTowns/itowns.git"
  },
  "license": "(CECILL-B OR MIT)",
  "bugs": {
    "url": "https://github.com/itowns/itowns/issues"
  },
  "homepage": "https://itowns.github.io/",
  "dependencies": {
    "@test-itowns-npm-token/geographic": "^2.46.0",
    "test-itowns-npm-token": "^2.46.0"
  }
}
